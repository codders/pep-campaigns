- if contacts.size > 0
  %table.contacts
    %thead
      %tr
        %th Name
        %th E-Mail
        %th Actions
    %tbody
      - contacts.each do |contact|
        %tr
          %td= contact.name
          %td= contact.email
          %td
            = link_to "Show Contact", admin_contact_path(contact)
- else
  %p None
